@article{Landauer1997,
abstract = {How do people know as much as they do with as little information as$\backslash$nthey get? The problem takes many forms; learning vocabulary from$\backslash$ntext is an especially dramatic and convenient case for research.$\backslash$nA new general theory of acquired similarity and knowledge representation,$\backslash$nlatent semantic analysis (LSA), is presented and used to successfully$\backslash$nsimulate such learning and several other psycholinguistic phenomena.$\backslash$nBy inducing global knowledge indirectly from local co-occurrence$\backslash$ndata in a large body of representative text, LSA acquired knowledge$\backslash$nabout the full vocabulary of English at a comparable rate to schoolchildren.$\backslash$nLSA uses no prior linguistic or perceptual similarity knowledge;$\backslash$nit is based solely on a general mathematical learning method that$\backslash$nachieves powerful inductive effects by extracting the right number$\backslash$nof dimensions (e.g., 300) to represent objects and contexts. Relations$\backslash$nto other theories, phenomena, and problems are sketched.},
author = {Landauer, Thomas K and Dumais, Susan T},
doi = {10.1037//0033-295X.104.2.211},
isbn = {0033-295X},
issn = {0033-295X},
journal = {Physcol. Rev.},
number = {2},
pages = {211--240},
title = {{A solution to Plate's problem: The latent semantic analysis theory of acquisition, induction, and representation of knowledge}},
volume = {104},
year = {1997}
}
