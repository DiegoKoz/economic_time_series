author = {Arakaki, Agust{\'{i}}n},
keywords = {Argentina,Metodolog{\'{i}}a de Estimaci{\'{o}}n de la Pobreza.,Necesidades B{\'{a}}sicas Insatisfechas,Pobreza},
pages = {269--290},
title = {{Trabajo y Sociedad Cuatro d{\'{e}}cadas de Necesidades B{\'{a}}sicas Insatisfechas en Argentina * Four decades of Unsatisfied Basic Needs in Argentina Quatro d{\'{e}}cadas de Necessidades B{\'{a}}sicas Insatisfeitas em Argentina}},
url = {http://www.unse.edu.ar/trabajoysociedad/27 ARAKAKI AGUSTIN CONICET NBI.pdf},
year = {2016}
}
